{
  "name": "ecran",
  "version": "0.1.0",
  "author": "Greg Konush",
  "license": "MIT",
  "packageManager": "pnpm@9.8.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:temporal dev:next dev:temporal-server",
    "dev:next": "next dev",
    "dev:temporal": "dotenvx run -f .env.local -- tsx watch ./src/temporal/worker.ts",
    "dev:temporal-server": "temporal server start-dev --ui-port 8080",
    "dev:temporal-port-forward": "kubectl port-forward -n temporal svc/temporal-frontend-headless 7233:7233",
    "build": "next build",
    "build:workflow": "tsx ./src/temporal/scripts/build-bundle.ts",
    "start:worker": "node src/temporal/worker.cjs",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "migrations": "tsx ./src/db/migrate.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@dotenvx/dotenvx": "^1.14.1",
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.11",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-table": "^8.20.5",
    "@temporalio/activity": "^1.11.1",
    "@temporalio/client": "^1.11.1",
    "@temporalio/worker": "^1.11.1",
    "@temporalio/workflow": "^1.11.1",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "esbuild-loader": "^4.2.2",
    "logrocket": "^9.0.0",
    "lucide-react": "^0.441.0",
    "next": "14.2.11",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.3.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-highlight": "^7.0.0",
    "resend": "^4.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.11",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2"
  }
}
